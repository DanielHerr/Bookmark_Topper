{
	"manifest_version": 2,
	"name": "Bookmark Topper",
	"version": "3.2.2.0",
	"version_name": "3.2 (2026.2.7, Mv2, Chromium 56-138)",
	"author": "Daniel Herr",
	"description": "Moves created bookmarks to the top of their folder.",
	"icons": { "128": "images/yellow_star.png" },
	"background": { "page": "background.html", "persistent": false },
	"offline_enabled": true,
	"permissions": [ "bookmarks", "storage", "notifications" ],
	"options_page": "Settings/Settings.html",
	"browser_action": { "default_title": "Bookmark Topper (enabled)" },
	"homepage_url": "https://danielherr.software/Bookmark_Topper",
	"update_url": "https://packages.danielherr.software/CRX/updates.xml",
	"minimum_chrome_version": "56"
}